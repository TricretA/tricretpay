{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
      { "source": "/api/(.*)", "destination": "/api/$1" },
      { "source": "/assets/(.*)", "destination": "/public/assets/$1" },
      { "source": "/(.*)", "destination": "/pages/$1" }
    ]
}