{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/pages/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    }
  ]
}