{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/pages/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    }
  ]
}